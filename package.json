{
  "name": "libraske-api",
  "description": "Libraske Service API",
  "version": "1.0.0",
  "private": true,
  "contributors": [
    "Vin√≠cius Guedes da Silva <viniciusguedes@lavid.ufpb.br>"
  ],
  "main": "src/server.js",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "setup-create-user": "ts-node-dev ./src/scripts/createUser",
    "setup-create-song": "ts-node-dev ./src/scripts/createSong",
    "dev": "cross-env NODE_ENV=dev ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "dth": "cross-env ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "test": "cross-env NODE_ENV=dev jest",
    "seed": "cross-env NODE_ENV=dev ts-node-dev src/database/seeds/DatabaseSeed.ts",
    "seed-dth": "cross-env ts-node-dev src/database/seeds/DatabaseSeed.ts",
    "prod": "tsc && cross-env NODE_ENV=dev ts-node dist/server.js",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/database/index.ts"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@types/amqplib": "^0.10.8",
    "@types/cors": "^2.8.10",
    "@types/express": "^5.0.6",
    "@types/figlet": "^1.5.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/jwk-to-pem": "^2.0.0",
    "@types/minimist": "^1.2.5",
    "@types/mocha": "^10.0.10",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "better-sqlite3": "^12.6.2",
    "cross-env": "^10.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.39.2",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-import-helpers": "^2.0.1",
    "eslint-plugin-prettier": "^5.5.5",
    "husky": "4.3.8",
    "jest": "^30.2.0",
    "jsdoc": "^4.0.5",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@faker-js/faker": "^10.2.0",
    "@types/express-serve-static-core": "^5.1.1",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.14.171",
    "amqplib": "^0.10.9",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.3",
    "chalk": "^5.6.2",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "cpf": "^2.0.1",
    "cpf-cnpj-validator": "^1.0.3",
    "cross-spawn": "^7.0.5",
    "dot-prop": "^10.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "figlet": "^1.5.0",
    "glob": "^13.0.0",
    "helmet": "^8.1.0",
    "jsdoc-plugin-typescript": "^3.2.0",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.5",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "pg": "^8.6.0",
    "qs": "^6.10.1",
    "reflect-metadata": "^0.2.2",
    "semver": "^7.5.2",
    "short-uuid": "^6.0.3",
    "sucrase": "^3.20.1",
    "ts-jest": "^29.4.6",
    "typeorm": "^0.3.24",
    "uuid": "^13.0.0",
    "xml2js": "0.6.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "**/xml2js": "0.5.0",
    "@types/express-serve-static-core": "^4.19.6",
    "**/semver": "^7.5.2",
    "glob": "^10.5.0"
  }
}
