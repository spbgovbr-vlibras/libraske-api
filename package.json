{
  "name": "libraske-api",
  "description": "Libraske Service API",
  "version": "1.0.0",
  "private": true,
  "contributors": [
    "Vin√≠cius Guedes da Silva <viniciusguedes@lavid.ufpb.br>"
  ],
  "main": "src/server.js",
  "license": "MIT",
  "engines": {
    "node": "^14"
  },
  "scripts": {
    "build": "tsc",
    "setup-create-user": "ts-node-dev ./src/scripts/createUser",
    "setup-create-song": "ts-node-dev ./src/scripts/createSong",
    "dev": "cross-env NODE_ENV=dev ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "test": "cross-env NODE_ENV=dev jest"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@types/amqplib": "^0.5.17",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.6",
    "@types/figlet": "^1.5.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/jwk-to-pem": "^2.0.0",
    "@types/mocha": "^8.2.2",
    "@types/mongodb": "^3.6.12",
    "@types/mongoose": "^5.10.5",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/uuidv4": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.2.5",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.4"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@types/jest": "^26.0.23",
    "amqplib": "^0.7.1",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "faker": "^5.5.3",
    "figlet": "^1.5.0",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "minimist": "^1.2.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "pg": "^8.6.0",
    "qs": "^6.10.1",
    "reflect-metadata": "^0.1.13",
    "short-uuid": "^4.1.0",
    "sqlite3": "^5.0.2",
    "ts-jest": "^27.0.3",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
